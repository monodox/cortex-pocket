{
  "buildCommand": "bash build.sh",
  "outputDirectory": "build/web",
  "routes": [
    {
      "src": "/flutter_bootstrap.js",
      "dest": "/flutter_bootstrap.js"
    },
    {
      "src": "/main.dart.js",
      "dest": "/main.dart.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}